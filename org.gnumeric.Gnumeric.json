{
    "id": "org.gnumeric.Gnumeric",
    "runtime": "org.gnome.Platform",
    "runtime-version": "49",
    "sdk": "org.gnome.Sdk",
    "command": "gnumeric",
    "rename-icon": "org.gnumeric.gnumeric",
    "copy-icon": true,
    "rename-desktop-file": "org.gnumeric.gnumeric.desktop",
    "rename-appdata-file": "org.gnumeric.gnumeric.appdata.xml",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=home",
        "--talk-name=org.gtk.vfs.*",
        "--filesystem=xdg-run/gvfs",
        "--filesystem=xdg-run/gvfsd"
    ],
    "modules": [
        "shared-modules/intltool/intltool-0.51.json",
        {
            "name": "gsf",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "/lib/pkgconfig",
                "/share/man",
                "/share/gtk-doc/html/gsf",
                "/share/thumbnailers",
                "/bin/gsf*",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libgsf/1.14/libgsf-1.14.54.tar.xz",
                    "sha256": "d18869264a2513cfb071712486d115dada064ff8a040265b49936bca06f17623",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libgsf",
                        "versions": {
                            "<": "1.15"
                        },
                        "stable-only": true
                    }
                }
            ]
        },
        {
            "name": "goffice",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "/share/gtk-doc",
                "/lib/pkgconfig",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/goffice/0.10/goffice-0.10.59.tar.xz",
                    "sha256": "b08f7173325594b71fbbea476a30b5b3120c3dadff5c0a26d140e4e524916622",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "goffice",
                        "versions": {
                            "<": "0.11"
                        },
                        "stable-only": true
                    }
                }
            ]
        },
        {
            "name": "psiconv",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "/share/man",
                "/share/psiconv",
                "/bin/psiconv*",
                "*.la"
            ],
            "build-options": {
                "cflags": "-fcommon"
            },
            "sources": [
                {
                    "type": "archive",
                    "url": "http://www.frodo.looijaard.name/system/files/software/psiconv/psiconv-0.9.9.tar.gz",
                    "sha256": "6d51fe79b502a1e277bea275a574ae2db5b1b9d7daef703a8fa3635ae02a8bb0"
                },
                {
                    "type": "patch",
                    "path": "patches/psiconv-fixes.patch"
                },
                {
                    "type": "shell",
                    "commands": [
                        "autoreconf -vfi"
                    ]
                }
            ]
        },
        {
            "name": "pxlib",
            "build-options": {
                "cflags": "-fpermissive"
            },
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.sourceforge.net/project/pxlib/pxlib/0.6.8/pxlib-0.6.8.tar.gz",
                    "sha256": "5f9b58a5c3d9e5e60faad16dfbc5172ee14ae57a978732378660c0f2c9e74ef9"
                }
            ]
        },
        {
            "name": "gnumeric",
            "buildsystem": "autotools",
            "cleanup": [
                "/include"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnumeric/1.12/gnumeric-1.12.59.tar.xz",
                    "sha256": "cb3750b176d641f9423df721b831658c829557552f8887fedf8a53d907eceb51",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "gnumeric",
                        "stable-only": true
                    }
                },
                {
                    "type": "patch",
                    "path": "patches/gnumerica-appstream.patch"
                }
            ]
        }
    ]
}
