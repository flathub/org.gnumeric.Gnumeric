{
    "id": "org.gnumeric.Gnumeric",
    "runtime": "org.gnome.Platform",
    "runtime-version": "42",
    "sdk": "org.gnome.Sdk",
    "command": "gnumeric",
    "rename-icon": "gnumeric",
    "copy-icon": true,
    "rename-desktop-file": "gnumeric.desktop",
    "rename-appdata-file": "gnumeric.appdata.xml",
    "finish-args" : [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=xdg-documents",
        "--talk-name=org.gtk.vfs.*",
        "--filesystem=xdg-run/gvfs",
        "--filesystem=xdg-run/gvfsd"
    ],
    "modules": [
        "shared-modules/intltool/intltool-0.51.json",
        {
            "name" : "gsf",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup" : [
                "/include",
                "/lib/pkgconfig",
                "/share/man",
                "/share/gtk-doc/html/gsf",
                "/share/thumbnailers",
                "/bin/gsf*",
                "*.la"
            ],
            "sources" : [
                {
                    "type" : "archive",
                    "url" : "https://download.gnome.org/sources/libgsf/1.14/libgsf-1.14.49.tar.xz",
                    "sha256": "e9ebe36688f010c9e6e40c8903f3732948deb8aca032578d07d0751bd82cf857"
                }
            ]
        },
        {
            "name" : "goffice",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup" : [
                "/include",
                "/share/gtk-doc",
                "/lib/pkgconfig",
                "*.la"
            ],
            "sources" : [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/goffice/0.10/goffice-0.10.51.tar.xz",
                    "sha256": "2e8410884d6a51ae00c9420928abfa73f7a552c1aa222371df4f2dc5acc3cd23"
                }
            ]
        },
        {
            "name" : "psiconv",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup" : [
                "/include",
                "/share/man",
                "/share/psiconv",
                "/bin/psiconv*",
                "*.la"
            ],
            "build-options": {
                "cflags": "-fcommon"
            },
            "sources" : [
                {
                    "type": "archive",
                    "url": "http://www.frodo.looijaard.name/system/files/software/psiconv/psiconv-0.9.9.tar.gz",
                    "sha256": "6d51fe79b502a1e277bea275a574ae2db5b1b9d7daef703a8fa3635ae02a8bb0"
                },
                {
                    "type" : "shell",
                    "commands" : [
                        "autoreconf -vfi"
                    ]
                }
            ]
        },
        {
            "name": "pxlib",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.sourceforge.net/project/pxlib/pxlib/0.6.8/pxlib-0.6.8.tar.gz",
                    "sha256": "5f9b58a5c3d9e5e60faad16dfbc5172ee14ae57a978732378660c0f2c9e74ef9"
                }
            ]
        },
        {
            "name": "gnumeric",
            "buildsystem": "autotools",
            "cleanup": [
                "/include"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnumeric/1.12/gnumeric-1.12.51.tar.xz",
                    "sha256": "a00e6c6e4ecddadabd9b0ae18013d7b05937fdcb8f9aad5a73b95923c7aeb1dd"
                },
                {
                    "type": "patch",
                    "path": "appdata.patch"
                }
            ]
        }
    ]
}
