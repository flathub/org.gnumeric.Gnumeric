{
    "id": "org.gnumeric.Gnumeric",
    "runtime": "org.gnome.Platform",
    "runtime-version": "45",
    "sdk": "org.gnome.Sdk",
    "command": "gnumeric",
    "rename-icon": "org.gnumeric.gnumeric",
    "copy-icon": true,
    "rename-desktop-file": "org.gnumeric.gnumeric.desktop",
    "rename-appdata-file": "org.gnumeric.gnumeric.appdata.xml",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--filesystem=xdg-documents",
        "--talk-name=org.gtk.vfs.*",
        "--filesystem=xdg-run/gvfs",
        "--filesystem=xdg-run/gvfsd"
    ],
    "modules": [
        "shared-modules/intltool/intltool-0.51.json",
        {
            "name": "gsf",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "/lib/pkgconfig",
                "/share/man",
                "/share/gtk-doc/html/gsf",
                "/share/thumbnailers",
                "/bin/gsf*",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/libgsf/1.14/libgsf-1.14.52.tar.xz",
                    "sha256": "9181c914b9fac0e05d6bcaa34c7b552fe5fc0961d3c9f8c01ccc381fb084bcf0",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "libgsf",
                        "versions": {
                            "<": "1.15"
                        },
                        "stable-only": true
                    }
                }
            ]
        },
        {
            "name": "goffice",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "/share/gtk-doc",
                "/lib/pkgconfig",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/goffice/0.10/goffice-0.10.56.tar.xz",
                    "sha256": "b8640a2fee0c0a57784b2a5b92944a2932c789db1039ddf5a269ad634796e7e2",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "goffice",
                        "versions": {
                            "<": "0.11"
                        },
                        "stable-only": true
                    }
                }
            ]
        },
        {
            "name": "psiconv",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "/share/man",
                "/share/psiconv",
                "/bin/psiconv*",
                "*.la"
            ],
            "build-options": {
                "cflags": "-fcommon"
            },
            "sources": [
                {
                    "type": "archive",
                    "url": "http://www.frodo.looijaard.name/system/files/software/psiconv/psiconv-0.9.9.tar.gz",
                    "sha256": "6d51fe79b502a1e277bea275a574ae2db5b1b9d7daef703a8fa3635ae02a8bb0"
                },
                {
                    "type": "shell",
                    "commands": [
                        "autoreconf -vfi"
                    ]
                }
            ]
        },
        {
            "name": "pxlib",
            "config-opts": [
                "--disable-static"
            ],
            "cleanup": [
                "/include",
                "*.la"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.sourceforge.net/project/pxlib/pxlib/0.6.8/pxlib-0.6.8.tar.gz",
                    "sha256": "5f9b58a5c3d9e5e60faad16dfbc5172ee14ae57a978732378660c0f2c9e74ef9"
                }
            ]
        },
        {
            "name": "gnumeric",
            "buildsystem": "autotools",
            "cleanup": [
                "/include"
            ],
            "post-install": [
                "appstream-util modify $FLATPAK_DEST/share/metainfo/org.gnumeric.gnumeric.appdata.xml id $FLATPAK_ID"
            ],
            "sources": [
                {
                    "type": "archive",
                    "url": "https://download.gnome.org/sources/gnumeric/1.12/gnumeric-1.12.56.tar.xz",
                    "sha256": "51a38f35ac5b0f71defa8b9e20bf2e08563798f1cb33379a9a17726fb1e3e1b2",
                    "x-checker-data": {
                        "type": "gnome",
                        "name": "gnumeric",
                        "stable-only": true
                    }
                }
            ]
        }
    ]
}
